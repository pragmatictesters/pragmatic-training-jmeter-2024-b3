String apiKey = &quot;${MAILOSAUR-API-KEY}&quot;;
String serverId = &quot;${MAILOSAUR-SERVER-ID}&quot;;

MailosaurClient mailosaur = new MailosaurClient(apiKey);

criteria = new SearchCriteria();
params = new MessageSearchParams();
params.withServer(serverId).withTimeout(12000);

criteria.withSentTo(&quot;${testEmail}&quot;);
Message message = mailosaur.messages().get(serverId, criteria);

String OTP=message.html().codes().get(0).value();
