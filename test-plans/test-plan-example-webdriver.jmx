        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="jp@gc - WebDriver Sampler : Login to the system">
          <stringProp name="WebDriverSampler.script">// Start the sample
//WDS.sampleResult.sampleStart()


// Locate the username input field and enter the username
username_field = WDS.browser.findElement(org.openqa.selenium.By.name(&apos;username&apos;))
username_field.sendKeys(&apos;Admin&apos;)

// Locate the password input field and enter the password
password_field = WDS.browser.findElement(org.openqa.selenium.By.name(&apos;password&apos;))
password_field.sendKeys(&apos;admin123&apos;)

// Locate the login button and click it
login_button = WDS.browser.findElement(org.openqa.selenium.By.tagName(&apos;button&apos;))
login_button.click()

// Wait for the next page to load (optional, adjust time as needed)
WDS.browser.manage().timeouts().implicitlyWait(5, java.util.concurrent.TimeUnit.SECONDS)

// End the sample
//WDS.sampleResult.sampleEnd()
</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">groovy</stringProp>
